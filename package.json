{
  "name": "ts-rpc",
  "version": "0.1.0",
  "description": "A lightweight, type-safe RPC library for TypeScript with Zod validation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example": "ts-node example-server/client-example.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "typescript",
    "rpc",
    "zod",
    "type-safe",
    "api",
    "validation"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.3",
    "express": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ts-rpc.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ts-rpc/issues"
  },
  "homepage": "https://github.com/yourusername/ts-rpc#readme"
}
